// finding smallest and second smallest element in arry 

class Barray19{

	public static void main(String[] args){

		int N=5;
	        int arr[]={2,4,3,5,6};
                int min=Integer.MAX_VALUE;
                int secondsm=0;

		for(int i=0;i<N;i++){

			if(arr[i]<min){
                                secondsm=min;
				min=arr[i];
                               }
		     }
		System.out.println("Second"+secondsm);
		System.out.println("min"+min);
	 }
   }

